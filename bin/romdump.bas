10 COLOR 15,1,1:SCREEN 0:WIDTH 80
20 CLEAR 300,&HA000
30 BLOAD"romdump.bin"
40 DEFUSR=&HA000
50 A=USR(0)
60 OPEN"romdump.rom" AS #1 LEN=256
70 FIELD #1,128 AS X$,128 AS Y$
80 FOR R=1 TO 64
90 E=(R-1)*256-24542
100 PRINT HEX$(E);" ";
110 A$=""
120 FOR J=0 TO 127
130 A$=A$+CHR$(PEEK(E+J))
140 NEXT J
150 LSET X$=A$
160 A$=""
170 FOR J=128 TO 255
180 A$=A$+CHR$(PEEK(E+J))
190 NEXT J
200 LSET Y$=A$
210 PUT #1,R
220 NEXT R
230 CLOSE #1
